# Docker ignore file for Task Tracker
# Ignore node_modules
node_modules/
client/node_modules/

# Ignore build artifacts
client/build/
dist/

# Ignore logs
logs/
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore environment files
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# Ignore uploads (these should be mounted as volumes)
uploads/

# Ignore git
.git/
.gitignore

# Ignore IDE files
.vscode/
.idea/
*.swp
*.swo

# Ignore OS generated files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# Ignore Docker files from being copied into image
Dockerfile*
docker-compose*.yml
.dockerignore

# Ignore development files
README.md
setup.sh
*.md

# Ignore test files
test/
tests/
__tests__/
*.test.js
*.spec.js

# Ignore coverage reports
coverage/

# Ignore temporary files
tmp/
temp/